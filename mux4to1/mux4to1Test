:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x589245c4e760 .scope module, "tb_mux4to1" "tb_mux4to1" 2 3;
 .timescale -9 -12;
v0x589245c64fe0_0 .var "d0", 0 0;
v0x589245c650a0_0 .var "d1", 0 0;
v0x589245c65170_0 .var "d2", 0 0;
v0x589245c65270_0 .var "d3", 0 0;
v0x589245c65340_0 .var "sel", 1 0;
v0x589245c65430_0 .net "y", 0 0, v0x589245c64e60_0;  1 drivers
S_0x589245c4e8f0 .scope module, "uut" "mux4to1" 2 8, 3 1 0, S_0x589245c4e760;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "sel";
    .port_info 1 /INPUT 1 "d0";
    .port_info 2 /INPUT 1 "d1";
    .port_info 3 /INPUT 1 "d2";
    .port_info 4 /INPUT 1 "d3";
    .port_info 5 /OUTPUT 1 "y";
v0x589245c14490_0 .net "d0", 0 0, v0x589245c64fe0_0;  1 drivers
v0x589245c64b10_0 .net "d1", 0 0, v0x589245c650a0_0;  1 drivers
v0x589245c64bd0_0 .net "d2", 0 0, v0x589245c65170_0;  1 drivers
v0x589245c64c70_0 .net "d3", 0 0, v0x589245c65270_0;  1 drivers
v0x589245c64d30_0 .net "sel", 1 0, v0x589245c65340_0;  1 drivers
v0x589245c64e60_0 .var "y", 0 0;
E_0x589245c4d5c0/0 .event anyedge, v0x589245c64d30_0, v0x589245c14490_0, v0x589245c64b10_0, v0x589245c64bd0_0;
E_0x589245c4d5c0/1 .event anyedge, v0x589245c64c70_0;
E_0x589245c4d5c0 .event/or E_0x589245c4d5c0/0, E_0x589245c4d5c0/1;
    .scope S_0x589245c4e8f0;
T_0 ;
    %wait E_0x589245c4d5c0;
    %load/vec4 v0x589245c64d30_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.0 ;
    %load/vec4 v0x589245c14490_0;
    %store/vec4 v0x589245c64e60_0, 0, 1;
    %jmp T_0.4;
T_0.1 ;
    %load/vec4 v0x589245c64b10_0;
    %store/vec4 v0x589245c64e60_0, 0, 1;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x589245c64bd0_0;
    %store/vec4 v0x589245c64e60_0, 0, 1;
    %jmp T_0.4;
T_0.3 ;
    %load/vec4 v0x589245c64c70_0;
    %store/vec4 v0x589245c64e60_0, 0, 1;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x589245c4e760;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x589245c4e760 {0 0 0};
    %vpi_call 2 13 "$monitor", "sel=%b | d0=%b d1=%b d2=%b d3=%b => y=%b", v0x589245c65340_0, v0x589245c64fe0_0, v0x589245c650a0_0, v0x589245c65170_0, v0x589245c65270_0, v0x589245c65430_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x589245c64fe0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c650a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c65170_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c65270_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x589245c65340_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c64fe0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x589245c650a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c65170_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c65270_0, 0, 1;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x589245c65340_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c64fe0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c650a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x589245c65170_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c65270_0, 0, 1;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x589245c65340_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c64fe0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c650a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589245c65170_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x589245c65270_0, 0, 1;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x589245c65340_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 20 "$display", "4-to-1 MUX test complete" {0 0 0};
    %vpi_call 2 21 "$finish" {0 0 0};
