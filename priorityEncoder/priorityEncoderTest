#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x628598ceb1a0 .scope module, "tb_priorityEncoder" "tb_priorityEncoder" 2 3;
 .timescale -9 -12;
v0x628598cfecd0_0 .var "in", 3 0;
v0x628598cfedc0_0 .net "out", 1 0, v0x628598cfead0_0;  1 drivers
v0x628598cfee90_0 .net "valid", 0 0, v0x628598cfebb0_0;  1 drivers
S_0x628598ceb330 .scope module, "uut" "priorityEncoder" 2 8, 3 1 0, S_0x628598ceb1a0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in";
    .port_info 1 /OUTPUT 2 "out";
    .port_info 2 /OUTPUT 1 "valid";
v0x628598ceb580_0 .net "in", 3 0, v0x628598cfecd0_0;  1 drivers
v0x628598cfead0_0 .var "out", 1 0;
v0x628598cfebb0_0 .var "valid", 0 0;
E_0x628598cb0ee0 .event anyedge, v0x628598ceb580_0;
    .scope S_0x628598ceb330;
T_0 ;
    %wait E_0x628598cb0ee0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x628598cfebb0_0, 0, 1;
    %load/vec4 v0x628598ceb580_0;
    %dup/vec4;
    %pushi/vec4 8, 7, 4;
    %cmp/z;
    %jmp/1 T_0.0, 4;
    %dup/vec4;
    %pushi/vec4 4, 3, 4;
    %cmp/z;
    %jmp/1 T_0.1, 4;
    %dup/vec4;
    %pushi/vec4 2, 1, 4;
    %cmp/z;
    %jmp/1 T_0.2, 4;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/z;
    %jmp/1 T_0.3, 4;
    %dup/vec4;
    %pushi/vec4 0, 0, 4;
    %cmp/z;
    %jmp/1 T_0.4, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x628598cfead0_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x628598cfebb0_0, 0, 1;
    %jmp T_0.6;
T_0.0 ;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x628598cfead0_0, 0, 2;
    %jmp T_0.6;
T_0.1 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x628598cfead0_0, 0, 2;
    %jmp T_0.6;
T_0.2 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x628598cfead0_0, 0, 2;
    %jmp T_0.6;
T_0.3 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x628598cfead0_0, 0, 2;
    %jmp T_0.6;
T_0.4 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x628598cfead0_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x628598cfebb0_0, 0, 1;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x628598ceb1a0;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x628598ceb1a0 {0 0 0};
    %vpi_call 2 13 "$monitor", "in=%b -> out=%b, valid=%b", v0x628598cfecd0_0, v0x628598cfedc0_0, v0x628598cfee90_0 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x628598cfecd0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x628598cfecd0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x628598cfecd0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x628598cfecd0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x628598cfecd0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 13, 0, 4;
    %store/vec4 v0x628598cfecd0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x628598cfecd0_0, 0, 4;
    %delay 10000, 0;
    %vpi_call 2 23 "$display", "Priority Encoder test done" {0 0 0};
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
